# API Configuration
# Base URL for the Laravel backend API
NEXT_PUBLIC_API_URL=https://api.example.com

# Optional: API version (defaults to v1)
NEXT_PUBLIC_API_VERSION=v1

# Cache Control (for testing)
# Set to 'false' to disable all API caching (useful for development/testing)
# Set to 'true' or leave empty to enable caching (recommended for production)
NEXT_PUBLIC_ENABLE_API_CACHE=true
